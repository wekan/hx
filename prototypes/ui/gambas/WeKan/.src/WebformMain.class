Public Sub WebMenuLogout_Click()

  ' Button Logout

  Dim Reponse As Integer

  Reponse = Message.Question("Do you really want to logout ?", "Yes", "No")

  Select Case Reponse
    Case 0
      ' No
      ModLogs.AddLogs("Logout - Reponse No")
      Return
    Case 1
      ' Yes
      ModLogs.AddLogs("Logout - Reponse Yes")

      Session.Abandon()
      WebForm.Goto(WebformLogin)
  End Select

Catch
  ModLogs.AddLogs("Error at WebButtonLogout_Click()")
  ModLogs.AddLogs("Error: " & Error.Text & "   " & "Where: " & Error.Where)

End

